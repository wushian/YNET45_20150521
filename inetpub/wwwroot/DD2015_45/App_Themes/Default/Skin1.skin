    BackColor="#368700" 
    Font-Bold="True" 
    ForeColor="White" />
  <PagerStyle 
    BackColor="#368700" 
    Font-Bold="True" 
    ForeColor="White"
    HorizontalAlign="Center" />
  <SelectedRowStyle 
    BackColor="#62A0AC" 
    ForeColor="White" />
  <EditRowStyle 
    BackColor="#128700" 
    ForeColor="White" />
  <RowStyle
   Height="24px" />
</asp:GridView>

<asp:GridView runat="server"
  BackColor="White" 
  BorderColor="#999999" 
  BorderStyle="Solid"
