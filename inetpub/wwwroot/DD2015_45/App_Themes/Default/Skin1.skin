<asp:TreeView 
  runat="server">
  <HoverNodeStyle 
    Font-Underline="False" 
    ForeColor="#FFFFFF"
    BackColor="#808080" />
  <NodeStyle 
    Font-Names="Arial" 
    Font-Size="11pt" 
    ForeColor="Black" 
    HorizontalPadding="0px" 
    NodeSpacing="0px" 
    VerticalPadding="0px" 
    Height="22px" />
  <ParentNodeStyle 
    Font-Bold="False" />
  <SelectedNodeStyle 
    Font-Underline="False" 
    ForeColor="#FFFFFF"
    BackColor="#000000" 
    HorizontalPadding="0px" 
    VerticalPadding="0px" />
</asp:TreeView>

<asp:Menu runat="server" SkinId="Menu2012" Orientation="Horizontal"
  Font-Names="Arial" DynamicHorizontalOffset="2"
  ForeColor="White" Font-Overline="False" 
  StaticEnableDefaultPopOutImage="False"
  StaticItemFormatString="&amp;nbsp;{0}&amp;nbsp;&amp;nbsp;"
  DynamicItemFormatString="&amp;nbsp;{0}&amp;nbsp;&amp;nbsp;"
  StaticSubMenuIndent="0px">
  <StaticMenuItemStyle ForeColor="White" HorizontalPadding="1px"
    Height="25px" CssClass="menu" />
  <StaticHoverStyle BackColor="Gray" ForeColor="White" />
  <StaticSelectedStyle BackColor="Black" ForeColor="White" />
  <DynamicMenuItemStyle HorizontalPadding="10px" Height="27px" BackColor="#368700" ForeColor="White" CssClass="menu" />
  <DynamicHoverStyle BackColor="Gray" Height="27px" ForeColor="White" />
  <DynamicSelectedStyle BackColor="Black" Height="27px" ForeColor="White" />
</asp:Menu>

<asp:Menu runat="server" Orientation="Horizontal"
  Font-Names="Arial" DynamicHorizontalOffset="2"
  ForeColor="White" Font-Overline="False" 
  StaticEnableDefaultPopOutImage="False"
  StaticItemFormatString="&amp;nbsp;&amp;nbsp;{0}&amp;nbsp;&amp;nbsp;"
  DynamicItemFormatString="&amp;nbsp;&amp;nbsp;{0}&amp;nbsp;&amp;nbsp;"
  StaticSubMenuIndent="0px">
  <StaticMenuItemStyle ForeColor="White" HorizontalPadding="5px"
    Height="35px" CssClass="menu" />
  <StaticHoverStyle BackColor="Gray" ForeColor="White" />
  <StaticSelectedStyle BackColor="Black" ForeColor="White" />
  <DynamicMenuItemStyle HorizontalPadding="10px" Height="37px" BackColor="#368700" ForeColor="White" CssClass="menu" />
  <DynamicHoverStyle BackColor="Gray" Height="37px" ForeColor="White" />
  <DynamicSelectedStyle BackColor="Black" Height="37px" ForeColor="White" />
</asp:Menu>

<asp:Calendar runat="server" BackColor="White" BorderColor="#404040"
  ForeColor="#FF5900" ShowGridLines="True">
  <DayHeaderStyle BackColor="#FFD7BA" ForeColor="Black"
    Font-Bold="False" />
  <DayStyle ForeColor="Black" />
  <NextPrevStyle ForeColor="#CCCCFF" />
  <OtherMonthDayStyle ForeColor="#999999" />
  <SelectedDayStyle BackColor="#368700" Font-Bold="True" ForeColor="White" />
  <SelectorStyle BackColor="#99CCCC" ForeColor="#000000" />
  <TitleStyle BackColor="#368700" BorderColor="#404040" Font-Bold="True" ForeColor="#FFFFFF" />
  <TodayDayStyle ForeColor="Black" />
  <WeekendDayStyle BackColor="#FF99CC" />
</asp:Calendar>


